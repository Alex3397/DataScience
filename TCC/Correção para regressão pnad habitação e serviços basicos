select * from pronto_para_regressao.habitacao_possuia_banheiro_de_uso_exclusivo_mil_domicilios;

drop table pronto_para_regressao.habitacao_possuia_banheiro_sanitario_ou_buraco_de_uso_exclusivo_mil_domicilios;
