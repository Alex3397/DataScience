select * from pronto_para_regressao.habitacao_telhado_telha_com_laje_de_concreto_mil_domicilios;

drop table pronto_para_regressao.habitacao_telhado_telha_sem_laje_de_concreto_mil_domicilios;
create table pronto_para_regressao.habitacao_telhado_telha_sem_laje_de_concreto_mil_domicilios ("Ano" numeric,
												"Acre" numeric,
												"Alagoas" numeric,
												"Amapá" numeric,
												"Amazonas" numeric,
												"Bahia" numeric,
												"Ceará" numeric,
												"Distrito Federal" numeric,
												"Espírito Santo" numeric,
												"Goiás" numeric,
												"Maranhão" numeric,
												"Mato Grosso" numeric,
												"Mato Grosso do Sul" numeric,
												"Minas Gerais" numeric,
												"Pará" numeric,
												"Paraíba" numeric,
												"Paraná" numeric,
												"Pernambuco" numeric,
												"Piauí" numeric,
												"Rio de Janeiro" numeric,
												"Rio Grande do Norte" numeric,
												"Rio Grande do Sul" numeric,
												"Rondônia" numeric,
												"Roraima" numeric,
												"Santa Catarina" numeric,
												"São Paulo" numeric,
												"Sergipe" numeric,
												"Tocantins" numeric);

insert into pronto_para_regressao.habitacao_telhado_telha_sem_laje_de_concreto_mil_domicilios ("Ano") values ('2016');
insert into pronto_para_regressao.habitacao_telhado_telha_sem_laje_de_concreto_mil_domicilios ("Ano") values ('2017');
